{
  "name": "tebble",
  "version": "0.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "eslint ./",
    "lint:fix": "eslint --fix ./",
    "build": "npx expo prebuild --clean",
    "build:fix": "md android\\app\\src\\main\\assets && npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Infernale-Stube/Tebble.git"
  },
  "author": "Infernale Stube",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Infernale-Stube/Tebble/issues"
  },
  "homepage": "https://github.com/Infernale-Stube/Tebble#readme",
  "dependencies": {
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "expo": "~49.0.6",
    "expo-font": "^11.4.0",
    "expo-splash-screen": "~0.20.4",
    "expo-status-bar": "~1.6.0",
    "expo-system-ui": "^2.4.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.3",
    "react-native-gesture-handler": "^2.12.1"
  },
  "engines": {
    "node": ">=18.16.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.9",
    "eslint": "^8.46.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1"
  },
  "private": true
}
